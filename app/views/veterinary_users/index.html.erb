<p id="notice"><%= notice %></p>

<h1>Listing Veterinary Users</h1>

<table>
  <thead>
    <tr>
      <th>Nameuser</th>
      <th>Passworduser</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @veterinary_users.each do |veterinary_user| %>
      <tr>
        <td><%= veterinary_user.nameuser %></td>
        <td><%= veterinary_user.passworduser %></td>
        <td><%= link_to 'Show', veterinary_user %></td>
        <td><%= link_to 'Edit', edit_veterinary_user_path(veterinary_user) %></td>
        <td><%= link_to 'Destroy', veterinary_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Veterinary user', new_veterinary_user_path %>
